<resources xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://developer.garmin.com/downloads/connect-iq/resources.xsd">
	<properties>
		<property id="macAddress" type="string"></property>
		<property id="showBoth" type="boolean"></property>
	</properties>
	
    <settings>
        <setting propertyKey="@Properties.macAddress" title="@Strings.macAddress" prompt="@Strings.macAddressPrompt">
            <settingConfig type="alphaNumeric" maxLength="17" />
        </setting>
        <setting propertyKey="@Properties.showBoth" title="@Strings.showBoth" prompt="@Strings.showBothPrompt">
            <settingConfig type="boolean" />
        </setting>
    </settings>
</resources>